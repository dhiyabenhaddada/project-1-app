<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dhiyabh.docway">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.dhiyabh.docway.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.dhiyabh.docway.SignUpActivity" />
        <activity
            android:name="com.example.dhiyabh.docway.MedAppActivity"
            android:label="@string/title_activity_med_app"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name="com.example.dhiyabh.docway.DoctorActivity" />
        <activity android:name="com.example.dhiyabh.docway.AppointmentActivity" />
        <activity android:name="com.example.dhiyabh.docway.CalendarActivity" />
    </application>

</manifest>